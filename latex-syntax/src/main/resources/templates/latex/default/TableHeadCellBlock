## If we're not on the last cell of the row then end with a "&"
#set ($currentCellBlock = $latex.block)
\textbf{${latex.processor.process($latex.block.getChildren())}}##
#if ("$!currentCellBlock.getNextSibling().class.getSimpleName()" == 'TableHeadCellBlock' || "$!currentCellBlock.getNextSibling().class.getSimpleName()" == 'TableCellBlock')
  ${SP}&${SP}##
#end